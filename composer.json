{
    "name": "php-hive/simple-url",
    "description": "SimpleURL is a lightweight PHP library for converting long URLs to short",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Pleskachov",
            "email": "art.pleskachov@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "type" : "library",
    "homepage": "https://github.com/PHP-Hive/SimpleUrl.git",
    "require" : {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "7.*",
        "ext-mbstring": "*"


    },
    "autoload" : {
        "psr-4" : {
            "PHPHive\\SimpleUrl\\" : "src/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        },
        "symfony": {
            "endpoint": [
                "https://api.github.com/repos/ufo-tech/recipes/contents/index.json?ref=main"
            ]
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
